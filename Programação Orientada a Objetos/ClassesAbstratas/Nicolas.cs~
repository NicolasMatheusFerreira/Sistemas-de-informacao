using System;

public sealed class Nicolas : Pessoa {
	
	public string nome;
	public string cpf;
	public string rg;
	public string curso;
	public int idade;

	public Nicolas() {
		
		this.nome = "N/A";
		this.cpf = "N/A";
		this.rg = "N/A";
		this.curso = "N/A";
		this.idade = 0;
		this.peso = 0.0;
		this.altura = 0.0;
		this.raca = "N/A";
		this.dataNascimento = "N/A";
	}

	public Nicolas(string nome, string cpf, string rg, string curso, int idade, string rua, string bairro, string cidade, string estado, int num, double peso, double altura, string raca, string dataNascimento) :base(peso, altura, raca, dataNascimento){

		this.nome = nome;
		this.cpf = cpf;
		this.rg = rg;
		this.curso = curso;
		this.idade = idade;
		this.peso = peso;
		this.altura = altura;
		this.raca = raca;
		this.dataNascimento = dataNascimento;
	}

	public void setNome(string nome) {
		this.nome = nome;
	}
	public string getNome() {
		return this.nome;
	}
	public void setCpf(string cpf) {
		this.cpf = cpf;
	}
	public string getCpf() {
		return this.cpf;
	}

	public void setRg(string rg) {
		this.rg = rg;
	}
	public string getRg() {
		return this.rg;
	}
	public void setCurso(string curso) {
		this.curso = curso;
	}
	public string getCurso() {
		return this.curso;
	}
	public void setIdade(int idade) {
		this.idade = idade;
	}
	public int getIdade() {
		return this.idade;
	}
}
