using System;

namespace Coercao {
	public class Coercao {
		public static void funcao2(string x) {
			Console.WriteLine(x);
		}
		public static void funcao(double x) {
			Console.WriteLine(x);
		}
	}

	class Program {
		static void Main(string[] args) {
			/*
           		Coercao.funcao(3.1415);
			Coercao.funcao((byte) 1);
			Coercao.funcao((short)2);
			Coercao.funcao('a');
			Coercao.funcao(3);
			Coercao.funcao(4L);
			Coercao.funcao(5.6F);
			*/
			Coercao.funcao2("a");
       		}
    	}
}
